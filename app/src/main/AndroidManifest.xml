<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ds.bahamut">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bahamut">
        <activity
            android:name=".sample.DialogsActivity"
            android:exported="true"
            android:label="@string/title_activity_dialogs"
            android:theme="@style/Theme.Bahamut" />
        <activity
            android:name=".sample.MenuListActivity"
            android:exported="true"
            android:label="@string/title_activity_menu_list"
            android:theme="@style/Theme.Bahamut" />
        <activity
            android:name=".sample.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Bahamut">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>